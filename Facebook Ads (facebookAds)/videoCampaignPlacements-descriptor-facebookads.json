    "datasources": {
        "facebookAds": {
            "service": "facebook_ads",
            "libraryAlias": "FacebooksAdsCampaignPlacements(93Days)",
            "name": "Facebooks Ads Campaign Placements (93 Days)",
            "description": "Retrieve the metrics of a specific Facebook Ads campaign broken down by ad set placements. Originates from the Gallery.",
            "type": "facebook",
            "format": "json",
            "refreshInterval": 14400,
            "props": {
                "endpoint_url": "https://graph.facebook.com/v2.7/{{ds.facebook_ads.campaignId}}/insights?time_range[since]={date.add(-93).format('yyyy-MM-dd')}&time_range[until]={date.today}&time_increment=1&fields=campaign_name,campaign_id,actions,spend,ctr,clicks,impressions,reach,cost_per_10_sec_video_view,video_10_sec_watched_actions,video_avg_pct_watched_actions,video_avg_sec_watched_actions&breakdowns=placement&limit=5000",
                "graphUrl": "https://graph.facebook.com/v2.7/{{ds.facebook_ads.campaignId}}/insights?time_range[since]={date.add(-93).format('yyyy-MM-dd')}&time_range[until]={date.today}&time_increment=1&fields=campaign_name,campaign_id,actions,spend,ctr,clicks,impressions,reach,cost_per_10_sec_video_view,video_10_sec_watched_actions,video_avg_pct_watched_actions,video_avg_sec_watched_actions&breakdowns=placement&limit=5000",
                "qtype": "Graph API"
            }
        }
    },
    "services": [{
        "service": "facebook_ads"
    }],
    "dsMappings": {
        "facebookAds": "66dbb540a08a06ad25ffdoogeraspilk"

    "datasources": {
        "facebookAds-01": {
            "service": "facebook_ads",
            "libraryAlias": "FacebooksAdsActiveCampaigns",
            "name": "Facebooks Ads Active Campaigns",
            "description": "Retrieve the list of active campaigns in your Facebook Ads account. Originates from the Gallery.",
            "type": "facebook",
            "format": "json",
            "refreshInterval": 14400,
            "props": {
                "endpoint_url": "https://graph.facebook.com/v2.6/{{ds.facebook_ads.accountId}}/campaigns?fields=id,name,created_time,effective_status&effective_status=['ACTIVE']",
                "graphUrl": "https://graph.facebook.com/v2.6/{{ds.facebook_ads.accountId}}/campaigns?fields=id,name,created_time,effective_status&effective_status=['ACTIVE']",
                "qtype": "Graph API"
            }
        },
        "facebookAds-02": {
            "service": "facebook_ads",
            "libraryAlias": "FacebooksAdsCampaignAdsetsBudget",
            "name": "Facebooks Ads Campaign Adsets Budgets",
            "description": "Retrieve the budget details of the Adsets in a Facebook Ads Campaign. Originates from the Gallery.",
            "type": "facebook",
            "format": "json",
            "refreshInterval": 14400,
            "props": {
                "endpoint_url": "https://graph.facebook.com/v2.6/{{ds.facebook_ads.campaignId}}/adsets?fields=id,name,lifetime_budget,daily_budget,budget_remaining",
                "graphUrl": "https://graph.facebook.com/v2.6/{{ds.facebook_ads.campaignId}/adsets?fields=id,name,lifetime_budget,daily_budget,budget_remaining",
                "qtype": "Graph API"
            }
        },
        "facebookAds-03": {
            "service": "facebook_ads",
            "libraryAlias": "FacebooksAdsCampaignMetrics",
            "name": "Facebooks Ads Campaign Metrics",
            "description": "Retrieve the daily metrics of a Facebook Ads Campaign for the last 90 days. Originates from the Gallery.",
            "type": "facebook",
            "format": "json",
            "refreshInterval": 14400,
            "props": {
                "endpoint_url": "https://graph.facebook.com/v2.6/{{ds.facebook_ads.campaignId}}/insights?date_preset=last_90_days&time_increment=1&fields=spend,actions,ctr,clicks,impressions&limit=100",
                "graphUrl": "https://graph.facebook.com/v2.6/{{ds.facebook_ads.campaignId}}/insights?date_preset=last_90_days&time_increment=1&fields=spend,actions,ctr,clicks,impressions&limit=100",
                "qtype": "Graph API"
            }
        },
        "googleAnalytics": {
            "service": "google_analytics",
            "libraryAlias": "GoogleAnalyticsCampaignMetrics",
            "name": "Google Analytics Campaign Metrics ({{ds.google_analytics.campaignName}})",
            "description": "Retrieve detailed Google AdWords metrics on your website's keyword performance through Google Analytics. Originates from the Gallery.",
            "type": "google_analytics",
            "format": "csv",
            "refreshInterval": 14400,
            "props": {
                "endpoint_url": "https://www.googleapis.com/analytics/v3/data/ga?ids=ga:{{ds.google_analytics.profileId}}&dimensions=ga:campaign,ga:date&metrics=ga:sessions,ga:bounces,ga:pageviews,ga:newusers,ga:bounceRate,ga:pageviewsPerSession,ga:sessionDuration,ga:ctr,ga:adClicks,ga:impressions&start-date={date.add(-90).format()}&end-date={date.today}&filters=ga:campaign=={hash.append('{{ds.google_analytics.campaignName}}').encodeURL().printDigest()}",
                "advancedQuery": "https://www.googleapis.com/analytics/v3/data/ga?ids=ga:{{ds.google_analytics.profileId}}&dimensions=ga:campaign,ga:date&metrics=ga:sessions,ga:bounces,ga:pageviews,ga:newusers,ga:bounceRate,ga:pageviewsPerSession,ga:sessionDuration,ga:ctr,ga:adClicks,ga:impressions&start-date={date.add(-90).format()}&end-date={date.today}&filters=ga:campaign=={hash.append('{{ds.google_analytics.campaignName}}').encodeURL().printDigest()}",
                "max_pages": "1",
                "mode": "Advanced",
                "profile": "{{ds.google_analytics.profileId}}",
                "timePeriod": "&start-date={date.add(-30).format()}&end-date={date.yesterday}"
            }
        }
    },

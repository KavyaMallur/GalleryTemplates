{
	"name" : "Swrve Revenue",
	"description": "Show daily revenue, average revenue per user and average revenue per paying user. Originates from the Gallery.",

	"datasources" : {
		"swrve-01" : {
			"type": "simple_rest",
			"format": "json",
			"name": "Swrve Daily Average Revenue per Paying User",
			"libraryAlias": "SwrveDailyARPPU",
			"refreshInterval": 14400,
			"description": "See your app's daily average revenue per paying user. Originates from the Gallery.",
			"props": {
				"endpoint_url": "https://dashboard.swrve.com/api/1/exporter/kpi/arppu_daily.json?api_key={{auth.swrve.apiKey}}&personal_key={{auth.swrve.personalKey}}&start={date.add(-30).format()}&granularity=day",
				"method": "GET"
			}
		},
		"swrve-02" : {
			"type": "simple_rest",
			"format": "json",
			"name": "Swrve Daily Average Revenue per User",
			"libraryAlias": "SwrveDailyARPU",
			"refreshInterval": 14400,
			"description": "See your app's daily average revenue per user. Originates from the Gallery.",
			"props": {
				"endpoint_url": "https://dashboard.swrve.com/api/1/exporter/kpi/arpu_daily.json?api_key={{auth.swrve.apiKey}}&personal_key={{auth.swrve.personalKey}}&start={date.add(-30).format()}&granularity=day",
				"method": "GET"
			}
		},
		"swrve-03" : {
			"type": "simple_rest",
			"format": "json",
			"name": "Swrve Daily Revenue",
			"libraryAlias": "SwrveDailyRevenue",
			"refreshInterval": 14400,
			"description": "See your app's daily revenue. Originates from the Gallery.",
			"props": {
				"endpoint_url": "https://dashboard.swrve.com/api/1/exporter/kpi/dollar_revenue.json?api_key={{auth.swrve.apiKey}}&personal_key={{auth.swrve.personalKey}}&start={date.add(-30).format()}&granularity=day",
				"method": "GET"
			}
		}
	},

	"dsMappings": {
		"swrve-01": "4ae4df24926a03f7cda4natceewzhere",
		"swrve-02": "f1e56f704bb784cd7766natceewzhere",
		"swrve-03": "24ae621d194d7a8efe4bnatceewzhere",

		

	},
	"services" : [
		{
			"service" : "swrve"
		}
	]


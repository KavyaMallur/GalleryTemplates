{
	"name" : "Swrve Engagement",
	"description": "Track average session length and average playtime for your mobile app. Originates from the Gallery.",

	"datasources" : {
		"swrve-01" : {
			"service": "swrve",
			"type": "simple_rest",
			"format": "json",
			"name": "Swrve Average Playtime",
			"libraryAlias": "SwrveAvgPlaytime",
			"refreshInterval": 14400,
			"description": "See the average amount of time users spend playing on your app. Originates from the Gallery.",
			"props": {
				"endpoint_url": "https://dashboard.swrve.com/api/1/exporter/kpi/avg_playtime.json?api_key={{auth.swrve.apiKey}}&personal_key={{auth.swrve.personalKey}}&start={date.add(-30).format()}&granularity=day",
				"method": "GET"
			}
		},
		"swrve-02" : {
			"service": "swrve",
			"type": "simple_rest",
			"format": "json",
			"name": "Swrve Daily Average Session Length",
			"libraryAlias": "SwrveDailyAverageSessionLength",
			"refreshInterval": 14400,
			"description": "See the daily average session length on your app. Originates from the Gallery.",
			"props": {
				"endpoint_url": "https://dashboard.swrve.com/api/1/exporter/kpi/avg_session_length.json?api_key={{auth.swrve.apiKey}}&personal_key={{auth.swrve.personalKey}}&start={date.add(-30).format()}&granularity=day",
				"method": "GET"
			}
		}
	},

	"dsMappings": {
		"swrve-01": "79d53f9b4b76c32330ebniktawuzhere",
		"swrve-02": "b43746e7a42542c3712dniktawuzhere"
	},

	"services" : [
		{
			"service" : "swrve"
		}
	]

}
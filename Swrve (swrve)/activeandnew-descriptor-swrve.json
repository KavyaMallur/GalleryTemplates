{
	"name" : "Swrve Active and New Users",
	"description": "Show number of active and new users. Originates from the Gallery.",

	"datasources" : {
		"swrve-01" : {
			"type": "simple_rest",
			"format": "json",
			"name": "Swrve Daily New Users",
			"libraryAlias": "SwrveNewUsersByyDay",
			"refreshInterval": 14400,
			"description": "See your website's daily new users. Originates from the Gallery.",
			"props": {
				"endpoint_url": "https://dashboard.swrve.com/api/1/exporter/kpi/new_users.json?api_key={{auth.swrve.apiKey}}&personal_key={{auth.swrve.personalKey}}&start={date.add(-30).format()}&granularity=day",
				"method": "GET"
			}
		},
		"swrve-02" : {
			"type": "simple_rest",
			"format": "json",
			"name": "Swrve Daily Active Users",
			"libraryAlias": "SwrveDailyActiveUsers",
			"refreshInterval": 14400,
			"description": "See your website's daily active users. Originates from the Gallery.",
			"props": {
				"endpoint_url": "https://dashboard.swrve.com/api/1/exporter/kpi/dau.json?api_key={{auth.swrve.apiKey}}&personal_key={{auth.swrve.personalKey}}&start={date.add(-30).format()}&granularity=day",
				"method": "GET"
			}
		}
	},

	"dsMappings": {
		"swrve-01": "32ff30a8464791b01a0bnatceewzhere",

{
    "connector": {
        "type": "simple_rest",
        "format": "json",
        "refresh_interval": 14400,
        "props": {
            "endpoint_url": "https://todoist.com/API/v7/sync?token=<YOUR-API-TOKEN>&resource_types=[\"projects\"]&sync_token='*'",
            "method": "get"
        }
    }
}


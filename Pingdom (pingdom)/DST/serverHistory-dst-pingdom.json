{
    "connector": {
        "type": "simple_rest",
        "format": "json",
        "refresh_interval": 14400,
        "props": {
            "endpoint_url": "https://api.pingdom.com/api/2.0/results/<yourServerID>",
            "method": "GET",
            "auth_method": "basic",
            "parameters": "[{\"name\":\"App-Key\",\"value\":\"<yourAPIKey>\",\"type\":\"header\"}]"
        }
    }
}


    },

    "services": [{
        "service": "harvest"
    }],

    "datasourceConfigModel": [{
        "type": "select",
        "id": "ds.harvest.projectId",
        "label": "Project",
        "description": "Select your project from the drop-down list.",
        "isRequired": true
    }],
    "queries": [{
        "errorMessage": "Make sure you have the correct Harvest username, password and subdomain. <br><br> When entering your Harvest subdomain, do <b>not</b> enter the whole URL: https://<b>OnlyEnterThisPart</b>.harvestapp.com.",
        "connector": {
            "service": "harvest",
            "type": "simple_rest",
            "format": "json",
            "props": {
                "auth_method": "basic",
                "basic_auth_username": "{{auth.harvest.userName}}",
                "basic_auth_password": "{{auth.harvest.password}}",
                "endpoint_url": "https://{{auth.harvest.subDomain}}.harvestapp.com/projects.json"
            }
        },
        "results": [{
            "propId": "ds.harvest.projectId",
            "propField": "options",
            "labels": "/project/name",
            "values": "/project/id"
        }]
    }]

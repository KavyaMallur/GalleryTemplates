    "datasources": {
        "harvest-01": {
            "service": "harvest",
            "type": "simple_rest",
            "format": "json",
            "name": "Harvest Projects",
            "libraryAlias": "HarvestProjects",
            "refreshInterval": 14400,
            "description": "Retrieve the projects in your harvest account. Originates from the Gallery.",
            "props": {
                "auth_method": "basic",
                "basic_auth_username": "{{auth.harvest.userName}}",
                "basic_auth_password": "{{auth.harvest.password}}",
                "endpoint_url": "https://{{auth.harvest.subDomain}}.harvestapp.com/projects.json",
                "method": "get",
                "parameters": "[]"
            }
        },
        "harvest-02": {
            "service": "harvest",
            "type": "simple_rest",
            "format": "json",
            "name": "Harvest Project Analysis (Lifespan)",
            "libraryAlias": "HarvestAnalysisLifespan",
            "refreshInterval": 14400,
            "description": "Retrieve the analysis of your Harvest project. Originates from the Gallery.",
            "props": {
                "auth_method": "basic",
                "basic_auth_username": "{{auth.harvest.userName}}",
                "basic_auth_password": "{{auth.harvest.password}}",
                "endpoint_url": "https://{{auth.harvest.subDomain}}.harvestapp.com/projects/{{ds.harvest.projectId}}/analysis.json?period=lifespan",
                "method": "get"
            }
        }
    },

    "dsMappings": {
        "harvest-01": "6f1451f8fb69e3cc884adoogeraspilk",

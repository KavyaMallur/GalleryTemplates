                "parameters": "[]"
            }
        },
        "harvest-02": {
            "service": "harvest",
            "type": "simple_rest",
            "format": "json",
            "name": "Harvest Tasks ({{ds.harvest.projectId}})",
            "libraryAlias": "HarvestTasks({{ds.harvest.projectId}})",
            "refreshInterval": 14400,
            "description": "Retrieve tasks for a selected project. Originates from the Gallery.",
            "props": {
                "auth_method": "basic",
                "basic_auth_username": "{{auth.harvest.userName}}",
                "basic_auth_password": "{{auth.harvest.password}}",
                "endpoint_url": "https://{{auth.harvest.subDomain}}.harvestapp.com/projects/{{ds.harvest.projectId}}/analysis.json?period=lifespan",
                "method": "get",
                "parameters": "[]"
            }
        }

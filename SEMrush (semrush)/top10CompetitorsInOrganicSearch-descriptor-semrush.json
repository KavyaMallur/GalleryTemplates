                "endpoint_url": "http://api.semrush.com/?type=domain_organic_organic&key={{auth.semrush.apiKey}}&display_limit=10&export_columns=Dn,Cr,Np,Or,Ot,Oc,Ad&domain={{ds.semrush.domain}}&database={{ds.semrush.database}}",

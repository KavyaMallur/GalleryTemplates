{
    "name": "SEMrush Top 10 Competitors in Organic Search",
    "description": "See the Top 10 Competitors in Organic Search based on the domains with common keywords. Domains with a high number of common keywords are considered competitors. Originates from the Gallery.",
    "datasources": {
        "semrush": {
            "service": "semrush",
            "type": "simple_rest",
            "format": "csv",
            "name": "SEMrush Top 10 Competitors in Organic Search",
            "libraryAlias": "SEMrushTop10CompetitorsinOrganicSearch",
            "refreshInterval": 86400,
            "description": "Retrieve your domain's top 10 competitors in organic search. Originates from the Gallery.",

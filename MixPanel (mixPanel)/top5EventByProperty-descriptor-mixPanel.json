{
    "name" : "Mixpanel Top 5 {{ds.mixpanel.event}} by {{ds.mixpanel.name}}",
    "description" : "Track your top five Mixpanel properties and the total number of their events. <br>Originates from the Gallery. (v1.1)",
    "datasources" : {
        "mixpanel" : {
            "service" : "mixpanel",
            "libraryAlias" : "MixpanelEventPropertiesToday",
            "name": "Mixpanel {{ds.mixpanel.event}} by {{ds.mixpanel.name}}(Today)",
            "description" : "Provide event properties for today on the event:Klip Added from Gallery. Originates from the Gallery.",
            "type" : "simple_rest",
            "format" : "json",
            "refreshInterval" : 14400,
            "props" : {
                "auth_method": "basic",
                "basic_auth_username": "{{auth.mixpanel.apiSecret}}",
                "endpoint_url": "https://mixpanel.com/api/2.0/events/properties?event={{ds.mixpanel.event}}&interval=0&name={{ds.mixpanel.name}}&type=general&unit=day"
            }
        }
    },

    "dsMappings" : {
        "mixpanel" : "380afc656132a5d991ceKavyaMallur9"
    },

    "services" : [
        {
            "service" : "mixpanel"
        }
    ],
    "datasourceConfigModel" : [
        {
            "type" : "select",
            "id" : "ds.mixpanel.event",
            "label" : "Select Event",
            "description":"Select the Mixpanel event and property that you are interested in tracking.",
            "onChangeQueries" : [ "q2" ]
        },
        {
            "type" : "select",
            "id" : "ds.mixpanel.name",
            "label" : "Enter Property"
        }
    ],
    "queries": [
        {
            "errorMessage":"Make sure you have the correct Mixpanel API Secret.",
            "connector": {
                "service": "mixpanel",
                "type": "simple_rest",
                "format": "json",
                "props": {
                    "auth_method": "basic",
                    "basic_auth_username": "{{auth.mixpanel.apiSecret}}",
                    "endpoint_url": "https://mixpanel.com/api/2.0/events/names/?"
                }
            },
            "results": [
                {
                    "propId" : "ds.mixpanel.event", 
                    "propField" : "options", 
                    "labels": "/",  
                    "values": "/" 
                }
            ]
        },
        {
            "id": "q2",
            "connector": {
                "service": "mixpanel",
                "type": "simple_rest",
                "format": "json",
                "props" : { 
                    "auth_method": "basic",
                    "basic_auth_username": "{{auth.mixpanel.apiSecret}}",
                    "endpoint_url": "https://mixpanel.com/api/2.0/events/properties/top?event={{ds.mixpanel.event}}&limit=100"
                }
            },
            "results": [
                {
                    "propId" : "ds.mixpanel.name", 
                    "propField" : "options", 
                    "labels": "kf:names(.)",  
                    "values": "kf:names(.)" 
                }
            ]
        }
    ],
    "uiVariables": [
        {
            "name":"Mixpaneltemp", 
            "value":"1"
        }
    ]
}
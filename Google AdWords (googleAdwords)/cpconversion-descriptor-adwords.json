            "type": "google_adwords",
            "format": "csv",
            "refreshInterval": 14400,
            "props": {
                "body": "__rdquery=SELECT AverageCpc,CostPerConversion\r\nFROM ACCOUNT_PERFORMANCE_REPORT\r\nDURING LAST_30_DAYS&__fmt=CSV&mode=Standard",

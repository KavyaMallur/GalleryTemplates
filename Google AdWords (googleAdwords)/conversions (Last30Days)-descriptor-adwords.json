    "datasources": {
        "googleAdwords-01": {
            "service": "google_adwords",
            "libraryAlias": "AdWordsCampaignList",
            "name": "Adwords Campaign List",
            "description": "See your AdWords campaigns and impressions. Originates from the Gallery.",
            "type": "google_adwords",
            "format": "csv",
            "refreshInterval": 14400,
            "props": {
                "body": "__rdquery=SELECT CampaignName,CampaignId,Impressions\r\nFROM CAMPAIGN_PERFORMANCE_REPORT\r\nDURING LAST_30_DAYS&__fmt=CSV&mode=Standard",
                "endpoint_url": "https://adwords.google.com/api/adwords/reportdownload/v201603",
                "method": "post",
                "parameters": "[{\"name\":\"developerToken\",\"value\":\"O5KbgkrEobequlPEVk-cLQ\",\"type\":\"header\"},{\"name\":\"clientCustomerID\",\"value\":\"{{ds.google_adwords.customerId}}\",\"type\":\"header\"}]"
            }
        },
        "googleAdwords-02": {
            "service": "google_adwords",
            "libraryAlias": "AdWordsCampaignPerformanceLast30Days",
            "name": "AdWords Campaign Performance (Last 30 Days)",
            "description": "See your key AdWords metrics (e.g. CPC, Clicks, Conversion Rate, etc.) by campaign for the last 30 days. Originates from the Gallery.",
            "type": "google_adwords",
            "format": "csv",
            "refreshInterval": 14400,
            "props": {
                "body": "__rdquery=SELECT AverageCpc,Amount,Period,Ctr,CampaignId,Clicks,ConversionRate,Cost,CostPerConversion,Impressions,ConversionValue,Conversions,Cost,Date\r\nFROM CAMPAIGN_PERFORMANCE_REPORT\r\nDURING LAST_30_DAYS&__fmt=CSV&mode=Custom",
                "endpoint_url": "https://adwords.google.com/api/adwords/reportdownload/v201603",
                "method": "post",
                "parameters": "[{\"name\":\"developerToken\",\"value\":\"O5KbgkrEobequlPEVk-cLQ\",\"type\":\"header\"},{\"name\":\"clientCustomerID\",\"value\":\"{{ds.google_adwords.customerId}}\",\"type\":\"header\"}]"
            }
        }
    },


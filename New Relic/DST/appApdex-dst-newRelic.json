{
  "connector": {
    "type": "simple_rest",
    "format": "json",
    "refresh_interval": 14400,
    "props": {
      "endpoint_url": "https://api.newrelic.com/v2/applications/<yourAppId>/metrics/data.xml?names[]=Apdex&values[]=score,s,f,t&from={date.tz(\"UTC\").addMinutes(-30).format(\"yyyy-MM-dd'T'HH:mm:ss'+00:00'\")}&to={date.tz(\"UTC\").today.format(\"yyyy-MM-dd'T'HH:mm:ss'+00:00'\")}",
      "method": "get",
      "parameters": "[{\"name\":\"X-Api-Key\",\"value\":\"<yourapikey>\",\"type\":\"header\"}]"
    }
  }
}


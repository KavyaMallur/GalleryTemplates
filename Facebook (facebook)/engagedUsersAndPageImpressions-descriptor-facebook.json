{
    "name": "Facebook Engaged Users and Page Impressions",
    "description": "Track the engagement and impressions on your facebook page for a selected period. Originates from the Gallery.",
    
    "datasources": {
        "facebook-01": {
            "service": "facebook",
            "libraryAlias": "FacebookInsightsMetrics(3Months)",
            "name": "Facebook Insights Metrics (3 Months)",
            "description": "Obtain your page Insights for over  3 months ago. Originates from the Gallery.",
            "type": "facebook",
            "format": "json",
            "refreshInterval": 14400,
            "props": {
                "endpoint_url": "https://graph.facebook.com/v2.7/{{ds.facebook.pageId}}/insights/page_engaged_users,page_impressions?since={date.addMonths(-2).startOfMonth.format()}&until={date.today}",

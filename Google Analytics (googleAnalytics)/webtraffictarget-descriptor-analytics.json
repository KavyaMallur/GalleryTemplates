    "datasources": {
        "googleAnalytics-01": {
            "service": "google_analytics",
            "libraryAlias": "GoogleAnalyticsDefaultChannelGroupingReportMTD",
            "name": "Google Analytics Default Channel Grouping Report (MTD)",
            "description": "Retrieve metrics on Google Analytics Default Channel Grouping for the current month till date. Originates from the Gallery.",
            "type": "google_analytics",
            "format": "csv",
            "refreshInterval": 14400,
            "props": {
                "endpoint_url": "https://www.googleapis.com/analytics/v3/data/ga?ids=ga:{{ds.google_analytics.profileId}}&metrics=ga:sessions,ga:bounceRate,ga:avgSessionDuration,ga:goalStartsAll,ga:pageviewsPerSession,ga:uniqueEvents,ga:sessionDuration,ga:bounces&dimensions=ga:channelGrouping&start-date={date.startOfMonth}&end-date={date.today}&max-results=10000",
                "max_pages": "1",
                "mode": "Simple",
                "profile": "{{ds.google_analytics.profileId}}",
                "timePeriod": "&start-date={date.startOfMonth}&end-date={date.today}"
            }
        },
        "googleAnalytics-02": {
            "service": "google_analytics",
            "libraryAlias": "GoogleAnalyticsDefaultChannelGroupingReportPrev MTD",
            "name": "Google Analytics Default Channel Grouping Report (Prev MTD)",
            "description": "Retrieve metrics on Google Analytics Default Channel Grouping for the previous  month. Originates from the Gallery.",
            "type": "google_analytics",
            "format": "csv",
            "refreshInterval": 14400,
            "props": {
                "endpoint_url": "https://www.googleapis.com/analytics/v3/data/ga?ids=ga:{{ds.google_analytics.profileId}}&metrics=ga:sessions,ga:bounceRate,ga:avgSessionDuration,ga:goalStartsAll,ga:pageviewsPerSession,ga:uniqueEvents,ga:sessionDuration,ga:bounces&dimensions=ga:channelGrouping&start-date={date.addMonths(-1).startOfMonth}&end-date={date.addMonths(-1).format()}&max-results=10000",
                "max_pages": "1",
                "mode": "Simple",
                "profile": "{{ds.google_analytics.profileId}}",
                "timePeriod": "&start-date={date.addMonths(-1).startOfMonth}&end-date={date.addMonths(-1).format()}"
            }
        },
        "googleAnalytics-03": {
            "service": "google_analytics",
            "libraryAlias": "GoogleAnalyticsDefaultChannelGroupingReportPrevMonth",
            "name": "Google Analytics Default Channel Grouping Report (Prev Month)",
            "description": "Retrieve metrics on Google Analytics Default Channel Grouping for the whole of last month. Originates from the Gallery.",
            "type": "google_analytics",
            "format": "csv",
            "refreshInterval": 14400,
            "props": {
                "endpoint_url": "https://www.googleapis.com/analytics/v3/data/ga?ids=ga:{{ds.google_analytics.profileId}}&metrics=ga:sessions,ga:bounceRate,ga:avgSessionDuration,ga:goalStartsAll,ga:pageviewsPerSession,ga:uniqueEvents,ga:sessionDuration,ga:bounces&dimensions=ga:channelGrouping&start-date={date.addMonths(-1).startOfMonth}&end-date={date.addMonths(-1).endOfMonth}&max-results=10000",
                "max_pages": "1",
                "mode": "Simple",
                "profile": "{{ds.google_analytics.profileId}}",
                "timePeriod": "&start-date={date.addMonths(-1).startOfMonth}&end-date={date.addMonths(-1).endOfMonth}"
            }
        }
    },

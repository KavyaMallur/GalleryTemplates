            "type": "google_analytics",
            "format": "csv",
            "refreshInterval": 14400,
            "props": {
                "endpoint_url": "https://www.googleapis.com/analytics/v3/data/ga?ids=ga:{{ds.google_analytics.profileId}}&dimensions=ga:landingPagePath,ga:date&metrics=ga:pageviews,ga:sessions,ga:goalCompletionsAll,ga:users,ga:bounces,ga:pageviewsPerSession,ga:timeOnPage&start-date={date.add(-280).format()}&end-date={date.today}&max-results=10000",
                "advancedQuery": "https://www.googleapis.com/analytics/v3/data/ga?ids=ga:{{ds.google_analytics.profileId}}&&dimensions=ga:landingPagePath,ga:date&metrics=ga:pageviews,ga:sessions,ga:goalCompletionsAll,ga:users,ga:bounces,ga:pageviewsPerSession,ga:timeOnPage&start-date={date.add(-280).format()}&end-date={date.today}&max-results=10000",
                "max_pages": "10",
                "mode": "Advanced",
                "profile": "{{ds.google_analytics.profileId}}",
                "timePeriod": "&start-date={date.add(-280).format()}&end-date={date.today}"
            }
        }
    },

  "datasources": {
    "googleAnalytics": {
      "service": "google_analytics",
      "libraryAlias": "SessionsLast30Days",
      "name": "GA Sessions (Last 30 Days)",
      "description": "See the number of visits to your website using mobile, tablet, and desktop devices for the last 30 days. Originates from the Gallery.",
      "type": "google_analytics",
      "format": "csv",
      "refreshInterval": 14400,
      "props": {
        "endpoint_url": "https://www.googleapis.com/analytics/v3/data/ga?ids=ga:{{ds.google_analytics.profileId}}&dimensions=ga:deviceCategory,ga:date&metrics=ga:sessions&start-date={date.add(-30).format()}&end-date={date.today}&max-results=10000",
        "max_pages": "1",
        "mode": "Advanced",
        "profile": "{{ds.google_analytics.profileId}}",
        "timePeriod": "&start-date={date.add(-30).format()}&end-date={date.today}"
      }
    }
  },

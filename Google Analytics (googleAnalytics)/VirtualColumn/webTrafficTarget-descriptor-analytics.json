{
    "name": "Google Analytics Web Traffic Targets (This Month)",
    "description": "Make sure you are hitting this monthâ€™s targets for driving traffic to your website. Monitor key stats on every channel source and compare it to the previous month. <br>Originates from the Gallery (v1.1).",
    "datasources": {
        "googleAnalytics-01": {
            "service": "google_analytics",
            "libraryAlias": "GoogleAnalyticsDefaultChannelGroupingReportMTD",
            "name": "Google Analytics Default Channel Grouping Report (MTD)",
            "description": "Retrieve metrics on Google Analytics Default Channel Grouping for the current month till date. Originates from the Gallery.",
            "type": "google_analytics",
            "format": "csv",
            "refreshInterval": 14400,
            "props": {
                "endpoint_url": "https://www.googleapis.com/analytics/v3/data/ga?ids=ga:{{ds.google_analytics.profileId}}&metrics=ga:sessions,ga:bounceRate,ga:avgSessionDuration,ga:goalStartsAll,ga:pageviewsPerSession,ga:uniqueEvents,ga:sessionDuration,ga:bounces&dimensions=ga:channelGrouping&start-date={date.startOfMonth}&end-date={date.today}&max-results=10000",
                "advancedQuery": "https://www.googleapis.com/analytics/v3/data/ga?ids=ga:{{ds.google_analytics.profileId}}&metrics=ga:sessions,ga:bounceRate,ga:avgSessionDuration,ga:goalStartsAll,ga:pageviewsPerSession,ga:uniqueEvents,ga:sessionDuration,ga:bounces&dimensions=ga:channelGrouping&start-date={date.startOfMonth}&end-date={date.today}&max-results=10000",
                "max_pages": "1",
                "mode": "Advanced",
                "profile": "{{ds.google_analytics.profileId}}",
                "timePeriod": "&start-date={date.startOfMonth}&end-date={date.today}"
            }
        },
        "googleAnalytics-02": {
            "service": "google_analytics",
            "libraryAlias": "GoogleAnalyticsDefaultChannelGroupingReportPrev MTD",
            "name": "Google Analytics Default Channel Grouping Report (Prev MTD)",
            "description": "Retrieve metrics on Google Analytics Default Channel Grouping for the previous  month. Originates from the Gallery.",
            "type": "google_analytics",
            "format": "csv",
            "refreshInterval": 14400,
            "props": {
                "endpoint_url": "https://www.googleapis.com/analytics/v3/data/ga?ids=ga:{{ds.google_analytics.profileId}}&metrics=ga:sessions,ga:bounceRate,ga:avgSessionDuration,ga:goalStartsAll,ga:pageviewsPerSession,ga:uniqueEvents,ga:sessionDuration,ga:bounces&dimensions=ga:channelGrouping&start-date={date.addMonths(-1).startOfMonth}&end-date={date.addMonths(-1).format()}&max-results=10000",
                "advancedQuery": "https://www.googleapis.com/analytics/v3/data/ga?ids=ga:{{ds.google_analytics.profileId}}&metrics=ga:sessions,ga:bounceRate,ga:avgSessionDuration,ga:goalStartsAll,ga:pageviewsPerSession,ga:uniqueEvents,ga:sessionDuration,ga:bounces&dimensions=ga:channelGrouping&start-date={date.addMonths(-1).startOfMonth}&end-date={date.addMonths(-1).format()}&max-results=10000",
                "max_pages": "1",
                "mode": "Advanced",
                "profile": "{{ds.google_analytics.profileId}}",
                "timePeriod": "&start-date={date.addMonths(-1).startOfMonth}&end-date={date.addMonths(-1).format()}"
            }
        },
        "googleAnalytics-03": {
            "service": "google_analytics",
            "libraryAlias": "GoogleAnalyticsDefaultChannelGroupingReportPrevMonth",
            "name": "Google Analytics Default Channel Grouping Report (Prev Month)",
            "description": "Retrieve metrics on Google Analytics Default Channel Grouping for the whole of last month. Originates from the Gallery.",
            "type": "google_analytics",
            "format": "csv",
            "refreshInterval": 14400,
            "props": {
                "endpoint_url": "https://www.googleapis.com/analytics/v3/data/ga?ids=ga:{{ds.google_analytics.profileId}}&metrics=ga:sessions,ga:bounceRate,ga:avgSessionDuration,ga:goalStartsAll,ga:pageviewsPerSession,ga:uniqueEvents,ga:sessionDuration,ga:bounces&dimensions=ga:channelGrouping&start-date={date.addMonths(-1).startOfMonth}&end-date={date.addMonths(-1).endOfMonth}&max-results=10000",
                "advancedQuery": "https://www.googleapis.com/analytics/v3/data/ga?ids=ga:{{ds.google_analytics.profileId}}&metrics=ga:sessions,ga:bounceRate,ga:avgSessionDuration,ga:goalStartsAll,ga:pageviewsPerSession,ga:uniqueEvents,ga:sessionDuration,ga:bounces&dimensions=ga:channelGrouping&start-date={date.addMonths(-1).startOfMonth}&end-date={date.addMonths(-1).endOfMonth}&max-results=10000",
                "max_pages": "1",
                "mode": "Advanced",
                "profile": "{{ds.google_analytics.profileId}}",
                "timePeriod": "&start-date={date.addMonths(-1).startOfMonth}&end-date={date.addMonths(-1).endOfMonth}"
            }
        }
    },
    "dsMappings": {
        "googleAnalytics-01": "cb7956e2ebb780503dcedoogeraspilk",
        "googleAnalytics-02": "d4e8c12a2963b2942efddoogeraspilk",
        "googleAnalytics-03": "dfe272e176a1b7304c32doogeraspilk"
    },
    "services": [{
        "service": "google_analytics"
    }],
    "datasourceConfigModel": [{
        "type": "select",
        "id": "ds.google_analytics.profileId",
        "label": "Analytics Account",
        "description": "Select an Analytics Account from the drop-down list.",
        "isRequired": true
    }],
    "queries": [{
        "errorMessage": "Here are two suggestions:<br><br>1. Make sure you have access to your company's Google Analytics account. <br><br>2. From time to time, your Google OAuth token will expire. This token allows you to safely bring your data into Klipfolio. <a href='https://support.klipfolio.com/hc/en-us/articles/215548958-OAuth-token-expired-for-connected-accounts' target='_blank'>Find out how to re-authorize your OAuth tokens</a>.",
        "connector": {
            "service": "google_analytics",
            "type": "simple_rest",
            "format": "json",
            "props": {
                "endpoint_url": "https://www.googleapis.com/analytics/v3/management/accounts/~all/webproperties/~all/profiles?fields=items(id%2Cname%2Cpermissions%2CwebPropertyId%2CwebsiteUrl)",
                "method": "GET"
            }
        },
        "results": [{
            "propId": "ds.google_analytics.profileId",
            "propField": "options",
            "labels": "/items/name",
            "values": "/items/id"
        }]
    }]
}

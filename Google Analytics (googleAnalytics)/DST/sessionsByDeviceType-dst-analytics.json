{
    "connector": {
        "type": "google_analytics",
        "format": "csv",
        "refresh_interval": 14400,
        "props": {
            "advancedQuery": "https://www.googleapis.com/analytics/v3/data/ga?ids=ga:<profileID>&dimensions=ga:deviceCategory,ga:date&metrics=ga:sessions&start-date={date.add(-365).format()}&end-date={date.today}&max-results=10000",
            "endpoint_url": "https://www.googleapis.com/analytics/v3/data/ga?ids=ga:<profileID>&dimensions=ga:deviceCategory,ga:date&metrics=ga:sessions&start-date={date.add(-365).format()}&end-date={date.today}&max-results=10000",
            "mode": "Advanced"
        }
    }
}
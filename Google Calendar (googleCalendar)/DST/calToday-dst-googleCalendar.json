{
  "connector": {
    "type": "simple_rest",
    "format": "json",
    "refresh_interval": 14400,
    "props": {
      "endpoint_url": "https://www.googleapis.com/calendar/v3/calendars/<yourCalendarID>/events?timeMin={date.format(\"yyyy-MM-dd'T'00:00:00Z\")}&timeMax={date.format(\"yyyy-MM-dd'T'23:59:59Z\")}&singleEvents=true"
    }
  }
}

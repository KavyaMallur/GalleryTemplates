    "services": 
    [{
        "service": "quintly"
    }, 
    {
        "service": "google_analytics"
    }],
    "datasourceConfigModel": 
    [{
        "type": "select",
        "id": "ds.google_analytics.profileId",
        "label": "Analytics Account",
        "isRequired": true,
        "onChangeQueries": ["q2"]
    }, 
    {
        "type": "select",
        "id": "ds.quintly.FBprofileId",
        "label": "Facebook Profile",
        "description": "Select your <b>Quintly Facebook Profile</b> and your <b>Quintly Twitter Profile</b>.",
        "onChangeQueries": ["q3"]
    }, 
    {
        "type": "select",
        "id": "ds.quintly.TWprofileId",
        "label": "Twitter Profile"
    },
    {
        "type": "text",
        "id": "ds.google_analytics.pagePath",
        "label": "Landing Page Path",
        "ghostText": "/index.php",
        "description": "Enter The landing page of your website below",
        "isRequired": true
    }],
    "queries": 
    [{
        "errorMessage": "Here are two suggestions:<br><br>1. Make sure you have access to your company's Google Analytics account. <br><br>2. From time to time, your Google OAuth token will expire. This token allows you to safely bring your data into Klipfolio. <a href='https://support.klipfolio.com/hc/en-us/articles/215548958-OAuth-token-expired-for-connected-accounts' target='_blank'>Find out how to re-authorize your OAuth tokens</a>.",
        "connector": {
            "service": "google_analytics",
            "type": "simple_rest",
            "format": "json",
            "props": {
                "endpoint_url": "https://www.googleapis.com/analytics/v3/management/accounts/~all/webproperties/~all/profiles?fields=items(id%2Cname%2Cpermissions%2CwebPropertyId%2CwebsiteUrl)",
                "method": "GET"
            }
        },
        "results": [{
            "propId": "ds.google_analytics.profileId",
            "propField": "options",
            "labels": "/items/name",
            "values": "/items/id"
        }]
    }, 
    {
        "id": "q2",
        "errorMessage": "Make sure you have the correct Quintly Client ID and API Secret.",
        "connector": {
            "service": "quintly",
            "type": "simple_rest",
            "format": "json",
            "props": {
                "endpoint_url": "https://api.quintly.com/v0.9/list-profiles",
                "basic_auth_username": "{{auth.quintly.clientId}}",
                "basic_auth_password": "{{auth.quintly.apiSecret}}"
            }
        },
        "results": [{
            "propId": "ds.quintly.FBprofileId",
            "propField": "options",
            "labels": "/data/platformUsername[preceding-sibling::platformType='facebook']",
            "values": "/data/id[following-sibling::platformType='facebook']"
        }]
    }, 
    {
        "id": "q3",
        "errorMessage": "Make sure you have the correct Quintly Client ID and API Secret.",
        "connector": {
            "service": "quintly",
            "type": "simple_rest",
            "format": "json",
            "props": {
                "endpoint_url": "https://api.quintly.com/v0.9/list-profiles",
                "basic_auth_username": "{{auth.quintly.clientId}}",
                "basic_auth_password": "{{auth.quintly.apiSecret}}"
            }
        },
        "results": [{
            "propId": "ds.quintly.TWprofileId",
            "propField": "options",
            "labels": "/data/platformUsername[preceding-sibling::platformType='twitter']",
            "values": "/data/id[following-sibling::platformType='twitter']"
        }]
    }]

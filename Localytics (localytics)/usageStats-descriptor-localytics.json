                "auth_method": "basic",
                "basic_auth_username": "{{auth.localytics.apiKey}}",
                "basic_auth_password": "{{auth.localytics.apiSecret}}",
                "endpoint_url": "https://api.localytics.com/v1/apps",
                "method": "get",
                "parameters": "[]"
            }
        },
        "results": [{
            "propId": "ds.localytics.appId",
            "propField": "options",
            "labels": "//*/apps/name",
            "values": "//*/apps/app_id"
        }]
    }]
